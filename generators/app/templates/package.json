{
  "name": "<%= name %>",
  "version": "0.0.0",
  "license": "MIT",
  "description": "<%= description %>",
  "scripts": {
    "lint": "eslint src",
    "test": "npm run test:once",
    "test:once": "ava src/**/*.test.js",
    "test:auto": "ava --watch src/**/*.test.js",
    "test:coverage": "nyc npm run test:once",
    "dev": "kotatsu serve --babel --config webpack.config.js --index index.html",
    "start": "node src/server.js",
    "build": "kotatsu build client --babel --minify --config webpack.config.js"
  },
  "ava": {
    "require": "babel-register"
  },
  "dependencies": {
    "app-module-path": "2.1.0",
    "ignore-styles": "5.0.1",
    "isomorphic-fetch": "2.2.1",
    "preact": "6.2.1",
    "preact-compat": "3.6.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "redux": "3.6.0",
    "redux-actions": "0.12.0",
    "redux-logger": "2.6.1",
    "redux-mock-store": "1.2.1",
    "redux-recycle": "1.2.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "ava": "0.16.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-3": "6.11.0",
    "css-loader": "0.25.0",
    "eslint": "3.6.0",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.3.0",
    "expect": "1.20.2",
    "extract-text-webpack-plugin": "1.0.1",
    "glob": "7.1.0",
    "kotatsu": "0.14.0",
    "node-sass": "3.10.0",
    "nyc": "8.3.0",
    "postcss-loader": "0.13.0",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1"
  }
}
